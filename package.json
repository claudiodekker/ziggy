{
    "name": "ziggy-js",
    "version": "0.9.1",
    "description": "Generates a Blade directive exporting all of your named Laravel routes. Also provides a nice route() helper function in JavaScript.",
    "keywords": [
        "laravel",
        "routes",
        "ziggy"
    ],
    "source": "src/js/route.js",
    "main": "dist/route.js",
    "umd:main": "dist/route.js",
    "unpkg": "dist/route.js",
    "module": "dist/route.m.js",
    "esmodule": "dist/route.es.js",
    "files": [
        "src/js",
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tightenco/ziggy.git"
    },
    "authors": [
        {
            "name": "Daniel Coulbourne",
            "email": "daniel@tighten.co"
        },
        {
            "name": "Jake Bathman",
            "email": "jake@tighten.co"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tightenco/ziggy/issues"
    },
    "homepage": "https://github.com/tightenco/ziggy#readme",
    "scripts": {
        "build": "microbundle --name route --format modern,es,umd",
        "test": "jest",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.10.4",
        "babel-preset-power-assert": "^3.0.0",
        "jest": "^26.1.0",
        "microbundle": "^0.12.3",
        "power-assert": "^1.6.1"
    },
    "dependencies": {
        "qs": "^6.8.0"
    },
    "engines": {
        "node": ">= 8.9"
    }
}
